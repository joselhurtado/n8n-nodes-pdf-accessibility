{
  "name": "n8n-nodes-pdf-accessibility",
  "version": "1.3.1",
  "description": "N8N community node for PDF accessibility remediation and WCAG compliance analysis",
  "keywords": [
    "n8n-community-node-package",
    "pdf",
    "accessibility",
    "wcag",
    "remediation",
    "compliance",
    "pdf-accessibility",
    "document-processing"
  ],
  "license": "MIT",
  "homepage": "https://github.com/joselhurtado/n8n-nodes-pdf-accessibility",
  "author": {
    "name": "Jose Hurtado",
    "email": "hello@hurtadojose.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joselhurtado/n8n-nodes-pdf-accessibility.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=8.6"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AnthropicApi.credentials.js",
      "dist/credentials/OpenAIApi.credentials.js",
      "dist/credentials/GoogleApi.credentials.js",
      "dist/credentials/CustomApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PdfAccessibility/PdfAccessibility.node.js",
      "dist/nodes/PdfAccessibility/PdfAccessibilityEnhanced.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "build:icons": "mkdir -p dist/icons && cp icons/pdf-accessibility.svg dist/ && cp -r icons/* dist/icons/",
    "verify:icons": "test -f dist/icons/pdf-accessibility.svg && echo 'Icons verified' || (echo 'Icons missing!' && exit 1)",
    "prebuild": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "prettier src --write",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run verify:icons",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^18.16.0",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^5.0.1",
    "jest": "^29.6.0",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "svgo": "^3.3.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ]
  }
}
